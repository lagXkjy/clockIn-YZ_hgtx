.activity {
  box-sizing: border-box;
  padding: 30rpx;
  background: #fff;
}

.activity-text {
  font-size: 26rpx;
  color: #000;
  line-height: 30rpx;
}

.activity-image-box {
  /* display: flex;
  align-items: center; */
