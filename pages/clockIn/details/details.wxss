.tea {
  width: 100%;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.tea-left {
  width: 76%;
  display: flex;
  align-items: center;
}

.tea-left-en {
  width: 69%;
  display: flex;
  align-items: center;
}

.tea-right {
  font-size: 28rpx;
  border: 1px solid #7c99d0;
  color: #7c99d0;
  padding: 0 10rpx;
  box-sizing: border-box;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 40rpx;
  border-radius: 5rpx;
}

.tea-image {
  border-radius: 50%;
  width: 99rpx;
  height: 99rpx;
  margin-right: 20rpx;
}

.tea-content {
  width: 76%;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #888;
}

.tea-uername {
  margin-bottom: 10rpx;
  font-size: 26rpx;
  align-items: center;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.tea-uername::after {
  content: '';
  display: block;
  clear: both;
}

.tea-t-image {
  float: left;
  display: flex;
}

.tea-vip {
  width: 28rpx;
  margin-left: 10rpx;
}

.tea-score {
  position: relative;
  display: flex;
}

.tea-score-list {
  width: 26rpx;
  height: 26rpx;
  margin-left: 6rpx;
  display: flex;
}

.point {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.tea-school {
  font-size: 24rpx;
  color: #000;
  box-sizing: border-box;
}

.tea-lable {
  width: 100%;
  overflow: hidden;
  display: flex;
  height: 40rpx;
  flex-wrap: wrap;
  margin-top: 15rpx;
}

.tea-lable-list {
  box-sizing: border-box;
  border: 1rpx solid #7c99d0;
  color: #7c99d0;
  margin-right: 10rpx;
  font-size: 24rpx;
  padding: 0 10rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 5rpx;
  line-height: 0;
}

.tea-tea {
  font-size: 24rpx;
  color: #888;
}

