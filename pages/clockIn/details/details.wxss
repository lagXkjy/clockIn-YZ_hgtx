/* pages/clockIn/details/details.wxss */

.header {
  position: relative;
  top: 0;
  left: 0;
  background: #7c9acf;
  height: auto;
}

.header-bg-image {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.header-box {
  position: relative;
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
}

.header-btn {
  height: 90rpx;
  box-sizing: border-box;
  width: 100%;
  color: #fff;
  position: relative;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  border: 2px solid #fff;
  overflow: hidden;
  border-radius: 8rpx;
  margin-top: 20rpx;
}

.header-btn-mc {
  width: 100%;
  height: 100%;
  background: #fff;
  opacity: 0.1;
  position: absolute;
  top: 0;
  left: 0;
}

.header-btn-image {
  width: 30rpx;
  margin-right: 30rpx;
}

.header-content {
  display: flex;
}

.header-image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 5rpx;
  margin-right: 40rpx;
}

.header-context {
  width: 530rpx;
  color: #fff;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
}

.header-name {
  font-size: 30rpx;
  font-weight: bold;
}

.header-num {
  font-size: 24rpx;
  margin: 20rpx 0;
}

.header-info {
  display: flex;
  align-items: center;
  font-size: 26rpx;
}

.header-info-image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}

.box {
  background: #fff;
  margin-top: 20rpx;
}

.add-theme {
  padding: 30rpx;
}

.add-theme-btn {
  border: 2px solid #7c9acf;
  border-radius: 8rpx;
  display: flex;
  height: 90rpx;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #7c9acf;
}

.add-theme-add {
  font-weight: bold;
  font-size: 40rpx;
  margin-right: 20rpx;
}

.theme-what {
  margin: 30rpx 0;
  color: #9a9a9a;
  font-size: 26rpx;
}

.theme-box {
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}

.theme {
  background: #fff;
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
}

.theme-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.theme-alltheme {
  font-size: 26rpx;
  color: #939393;
}

.theme-left {
  display: flex;
  align-items: center;
  color: #222;
  font-size: 26rpx;
  line-height: 26rpx;
}

.theme-left-text {
  line-height: 0rpx; /* 鬼知道是为什么 */
}

.theme-left-image {
  width: 30rpx;
  margin-right: 12rpx;
}

.theme-left-g {
  font-size: 28rpx;
  color: #939393;
  margin: 0 14rpx;
  line-height: 28rpx;
}

.theme-title {
  margin: 40rpx 0;
  color: #2e2e2e;
  font-weight: bold;
  font-size: 30rpx;
}

.theme-image {
  width: 90%;
  margin: 0 auto;
  display: block;
}

.theme-btn-box {
  display: flex;
  justify-content: center;
}

.theme-btn {
  background: #7c9acf;
  color: #fff;
  height: 60rpx;
  padding: 0 46rpx;
  font-size: 26rpx;
  border-radius: 8rpx;
  margin-top: 35rpx;
  display: flex;
  align-items: center;
}

.tab {
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
  height: 80rpx;
  color: #939393;
  font-size: 26rpx;
  display: flex;
  justify-content: space-around;
  box-sizing: border-box;
}

.tab-list {
  height: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.tab-list-select {
  color: #7c9acf;
  border-bottom: 2px solid #7c9acf;
}

modular-title {
  display: block;
  margin-top: 20rpx;
  border-top: 1px solid #f0f0f0;
  border-bottom: 1px solid #f0f0f0;
}

.height {
  height: 20rpx;
}

.raking-prompt {
  background: #fefaea;
  color: #343434;
  font-size: 26rpx;
  padding: 30rpx;
}

.raking-me {
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
  background: #fff;
  margin-top: 20rpx;
  border-bottom: 1px solid #efefef;
  border-top: 1px solid #efefef;
}

.no_ranking {
  padding: 30rpx;
  font-size: 30rpx;
  text-align: center;
}
.no_ranking view{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-bottom: 47rpx;
  box-sizing: border-box;
}
.no_ranking view image {
  width: 192rpx;
  height: 215rpx;
}

.raking-me-left {
  border-left: 2px solid #7c9acf;
  padding-left: 14rpx;
}

.raking-me-raking {
  font-weight: bold;
  color: #ee794d;
}

.raking-box {
  padding: 0 30rpx;
  background: #fff;
}

.raking-list {
  border-bottom: 1px solid #efefef;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
}

.raking-score {
  font-size: 26rpx;
  color: #232626;
}

.raking-left {
  width: 80%;
  display: flex;
  align-items: center;
}

.raking-raking {
  width: 45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #252525;
}

.raking-raking image {
  width: 100%;
}

.raking-image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin: 0 20rpx;
}

.raking-context {
  width: 90%;
}

.raking-name {
  color: #7c9acf;
  font-size: 26rpx;
  margin-bottom: 20rpx;
  word-break: break-all;
  word-wrap: break-word;
}

.raking-day {
  font-size: 24rpx;
  color: #8f8f8f;
}

/* 邀请 */
.invite {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 0 75rpx;
  overflow: hidden;
}

.invite-en {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
